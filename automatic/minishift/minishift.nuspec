<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>minishift</id>
    <version>{{PackageVersion}}</version>
    <packageSourceUrl>https://github.com/abejenaru/chocolatey-packages</packageSourceUrl>
    <owners>Andrei Bejenaru</owners>
    <title>Minishift: Run OpenShift locally</title>
    <authors>Google</authors>
    <projectUrl>https://openshift.org</projectUrl>
    <iconUrl>http://cdn.rawgit.com/abejenaru/chocolatey-packages/master/icons/minishift.png</iconUrl>
    <copyright>2016 RedHat</copyright>
    <licenseUrl>https://github.com/minishift/minishift/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectSourceUrl>https://github.com/minishift/minishift</projectSourceUrl>
    <docsUrl>https://openshift.org/docs</docsUrl>
    <bugTrackerUrl>https://github.com/minishift/minishift/issues</bugTrackerUrl>
    <tags>openshift kubernetes docker rkt containers devops development</tags>
    <summary>Minikube is a tool that makes it easy to run OpenShift Origin locally.</summary>
    <description>Minikube runs a single-node OpenShift Origin inside a VM on your laptop for users looking to try out Kubernetes or develop with it day-to-day.</description>
    <releaseNotes>https://github.com/minishift/minishift/blob/v{{PackageVersion}}/CHANGELOG.md</releaseNotes>
    <dependencies>
      <dependency id="openshift-cli" version="1.4.1"/>
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
