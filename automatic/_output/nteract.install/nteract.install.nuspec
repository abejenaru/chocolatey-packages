<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>nteract.install</id>
    <version>0.0.15</version>
    <packageSourceUrl>https://github.com/abejenaru/chocolatey-packages</packageSourceUrl>
    <owners>Andrei Bejenaru</owners>
    <title>nteract (Install)</title>
    <authors>nteract contributors</authors>
    <projectUrl>https://nteract.io/</projectUrl>
    <iconUrl>http://cdn.rawgit.com/abejenaru/chocolatey-packages/master/icons/nteract.png</iconUrl>
