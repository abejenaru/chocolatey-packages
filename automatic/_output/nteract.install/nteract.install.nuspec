    <licenseUrl>https://github.com/nteract/nteract/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectSourceUrl>https://github.com/nteract/nteract</projectSourceUrl>
